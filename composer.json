{
    "name": "imagina/isite-module",
    "description": "custom settings of the site",
    "type": "asgard-module",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "composer/installers": "~1.0",
        "imagina/core-module": "v10.x-dev",
        "imagina/ihelpers-module": "v10.x-dev",
        "imagina/igamification-module": "v10.x-dev",
        "livewire/livewire": "^2.11",
        "jantinnerezo/livewire-alert": "^2.2",
        "imagina/laravel-responsecache": "^7.4",
        "kalnoy/nestedset": "6.0",
        "hisorange/browser-detect": "^4.4.0",
        "maatwebsite/excel": "^3.1",
        "stancl/tenancy": "^3.7",
        "barryvdh/laravel-dompdf": "^2.0",
        "imagina/ischedulable-module": "^v10.x-dev",
        "imagina/ifillable-module": "^v10.x-dev",
        "spatie/laravel-sitemap": "^6.3",
        "spatie/robots-txt": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10",
        "orchestra/testbench": "^8.5"
    },
    "autoload-dev": {
        "psr-4": {
            "Modules\\Isite\\": ".",
            "Modules\\": "Modules/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Nicolas Widart",
            "email": "n.widart@gmail.com"
        }
    ],
    "scripts": {
        "post-install-cmd": [
            "php artisan module:seed isite"
        ]

    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0.x-dev",
            "dev-v10.x": "10.0.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Modules\\Isite\\": ""
        }
    }
}
